{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": ["eslint:recommended","google","prettier"],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "document":true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "prettier",
      "html",        
      "react"
  ],
  "rules": {
      "prettier/prettier": "error",
      "max-len":"off",
      "require-jsdoc":"off",
      "no-unused-vars":"off",
      "no-undef":"off",
      "no-console":"off"
  }
}
